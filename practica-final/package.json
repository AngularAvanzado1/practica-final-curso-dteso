{
  "name": "practica-final",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "e2e:practica-final": "ng e2e practica-final-e2e --watch",
    "test:services": "ng test shared-api",
    "test:pages": "ng test practica-final",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "build:pwa": "ng build --prod",
    "localhost-prod": "angular-http-server -p 8080 --path ./dist/practica-final/browser",
    "start:ssr": "npm run build:ssr && npm run serve:ssr",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run practica-final:server:production --bundleDependencies all",
    "compile:server": "webpack --config webpack.server.config.js --progress --  colors",
    "serve:ssr": "node dist/practica-final/server/main.js",
    "i18n:practica-final": "ng xi18n practica-final --output-path src/locale",
    "build:practica-final-es": "ng build practica-final --configuration=production-es",
    "start:practica-final-es": "npm run build:practica-final-es && angular-http-server --path ./dist/apps/practica-final/es"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/platform-server": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@angular/service-worker": "^9.1.0",
    "@ngrx/effects": "9.1.0",
    "@ngrx/entity": "9.1.0",
    "@ngrx/router-store": "9.1.0",
    "@ngrx/store": "9.1.0",
    "@nguniversal/express-engine": "^9.1.1",
    "@nrwl/angular": "9.3.0",
    "angular-http-server": "^1.9.0",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "rxjs": "~6.5.4",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@ngrx/schematics": "9.1.0",
    "@ngrx/store-devtools": "9.1.0",
    "@nguniversal/builders": "^9.1.1",
    "@nrwl/cypress": "9.3.0",
    "@nrwl/jest": "9.3.0",
    "@nrwl/workspace": "9.3.0",
    "@types/express": "^4.17.0",
    "@types/jest": "25.1.4",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "jest": "25.2.3",
    "jest-preset-angular": "8.1.2",
    "prettier": "2.0.4",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~3.8.3"
  }
}
